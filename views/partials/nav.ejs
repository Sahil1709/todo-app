<body>
    <div class="container">
      <header class="d-flex flex-wrap align-items-center justify-content-center justify-content-md-between py-3 mb-4 border-bottom">
        <nav class="navbar navbar-light bg-light">
          <span class="navbar-brand mb-0 h1"><%= title%></span>
        </nav>
        <ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
          <li><a href="/" class="nav-link px-2 link-secondary">Home</a></li>
          <li><a href="./add-items" class="nav-link px-2 link-dark">Add Items</a></li>
          <li><a href="./error404" class="nav-link px-2 link-dark">Error</a></li>
        </ul>
        <form class="form-inline my-2 my-lg-0" style="display: inherit;">
          <input id="search" class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
          <button id="searchbtn" class="btn s-btn" type="submit">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
              <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
            </svg>
          </button>
        </form>
        
      </header>
<script>
  searchbtn.onclick = function (e){
    e.preventDefault()
    var searchitem = document.getElementById('search').value
    console.log(searchitem)
    location.href = "../search/"+searchitem;
  }
</script>